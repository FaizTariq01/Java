import java.util.*;
class Main{
    public static void main(String[] args){
        System.out.println("enter number");
        Scanner sc=new Scanner(System.in);
        int a= sc.nextInt();
        for(int i=2;i<=a;i++){
            if(a%i==0){
                System.out.println("the number is not prime");
                return;
            }
            else{
                System.out.println("the no is prime");
                return;
            }
        }
        
    }

}
